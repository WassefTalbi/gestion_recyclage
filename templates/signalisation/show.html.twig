{% extends 'base.html.twig' %}

{% block title %}Signalisation{% endblock %}

{% block body %}
    <h1>Signalisation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ signalisation.id }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ signalisation.description }}</td>
            </tr>
            <tr>
                <th>DateSignal</th>
                <td>{{ signalisation.dateSignal ? signalisation.dateSignal|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Urlphoto</th>
                <td>{{ signalisation.urlphoto }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_signalisation_index') }}">back to list</a>

    <a href="{{ path('app_signalisation_edit', {'id': signalisation.id}) }}">edit</a>

    {{ include('signalisation/_delete_form.html.twig') }}
{% endblock %}
